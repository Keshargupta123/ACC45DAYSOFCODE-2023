#include <stdio.h>

int main() {
     float num1,num2;
     printf("Enter the first number :\n");
     scanf("%f",&num1);
     printf("Enter the second number:\n");
     scanf("%f",&num2);
     float a,S,S1;
     a=num1+num2;
     printf("%f",&a);
     S=num1-num2;
     S1=num2-num1;
     num1>num2?S:S1;
     return 0;
}
